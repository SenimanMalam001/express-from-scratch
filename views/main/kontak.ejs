<!DOCTYPE html>
<html lang="en" id="home">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=devide-width, initial-scale=1">

  <title>Portfolio</title>

  <link rel="stylesheet" href="css/bootstrap.css">
  <link rel="stylesheet" href="css/style.css">
   
</head>
<body>
  <!-- contact -->
  <section class="contact" id="contact">
    <div class="container">
      <div class="row">
        <div class="col-sm-12 text-center">
          <h2>Contact</h2>
          <hr>
        </div>
      </div>

      <div class="row">
        <div class="col-sm-8 col-sm-offset-2">
          <form action="/post" method="POST">
            <div class="form-group">
              <label for="nama" class="">Nama</label>
              <input name="nama" type="text" id="nama" class="form-control" placeholder="Masukan Nama">
            </div>
            
            <div class="form-group">
              <label for="email" class="">Email</label>
              <input name="email" type="email" id="nama" class="form-control" placeholder="Masukan Email">
            </div>
            
            <div class="form-group">
              <label for="telp" class="">Telpon</label>
              <input name="telpon" type="tel" id="nama" class="form-control" placeholder="Masukan Telpon">
            </div>     

            <button type="submit" class="btn btn-primary">
              Succes-Page
            </button>
          </form>
        </div>
      </div>
    </div>
  </section>
  <!-- akhircontact -->
</body>
</html>